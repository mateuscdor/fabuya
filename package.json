{
  "name": "fabuya",
  "version": "0.1.0-beta.7",
  "description": "Baileys Whatsapp library wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "example.basic": "npx ts-node examples/basic.ts",
    "build": "npx swc src -d dist",
    "build.baileys": "npm install --prefix Baileys -D",
    "build.declaration": "npx tsc --emitDeclarationOnly",
    "check": "npx tsc --noEmit",
    "postinstall": "node postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HanzHaxors/fabuya.git"
  },
  "keywords": [
    "whatsapp",
    "api",
    "baileys"
  ],
  "author": "Hanz <hanz@godot.id>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/HanzHaxors/fabuya/issues"
  },
  "homepage": "https://github.com/HanzHaxors/fabuya#readme",
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@hapi/boom": "^9.1.4",
    "axios": "^0.26.1",
    "curve25519-js": "^0.0.4",
    "futoin-hkdf": "^1.5.0",
    "libsignal": "^2.0.1",
    "music-metadata": "^7.12.2",
    "node-cache": "^5.1.2",
    "pino": "^7.9.2",
    "protobufjs": "~6.11.2",
    "qrcode-terminal": "^0.12.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.197",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "files": [
    "/dist",
    "/Baileys",
    "/Baileys/lib",
    "postinstall.js"
  ]
